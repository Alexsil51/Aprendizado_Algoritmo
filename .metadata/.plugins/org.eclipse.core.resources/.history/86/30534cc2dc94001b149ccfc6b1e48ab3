package Exercicios;

public class Teste2 {
	
	  public static void main(String args[]) {
		  int contador = 0;
			while(contador <= 50){
				contador++;
				if(contador%2==0)
					System.out.println("Numero Par com WHILE: " + contador);
			}
			int j = 0;
			do{
				j++;
				if(j%2==0)
					System.out.println("Numero Par com DO: " + j);
			}while(j <= 50);
	        }
	    }
	


